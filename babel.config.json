{
    "env": {
        "test": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "ie": "11",
                            "edge": "12",
                            "firefox": "50",
                            "chrome": "50",
                            "safari": "9",
                            "ios": "9"
                        }
                    }
                ]
            ],
            "plugins": ["@babel/plugin-proposal-class-properties"]
        },
        "development": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                            "ie": "11",
                            "edge": "12",
                            "firefox": "50",
                            "chrome": "50",
                            "safari": "9",
                            "ios": "9"
                        }
                    }
                ]
            ],
            "plugins": ["@babel/plugin-proposal-class-properties"]
        }
    }
    
}
